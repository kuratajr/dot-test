{
   "background": {
      "persistent": false,
      "scripts": [ "background.js", "ping.js", "notifications.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "images/128x128.png",
         "16": "images/16x16.png",
         "32": "images/32x32.png",
         "48": "images/48x48.png"
      },
      "default_popup": "rewards_popup.html"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "is_default": true,
         "prepopulated_id": 1,
         "search_url": "https://www.bing.com/search?PC=U523&q={searchTerms}",
         "suggest_url": "{bing:baseURL}qbox?query={searchTerms}&language={language}&PC=U523&{bing:partner}{bing:cvid}{bing:ig}{google:inputType}{google:cursorPosition}{google:pageClassification}{bing:features}{bing:flights}"
      }
   },
   "default_locale": "en",
   "description": "__MSG_RewardsDesc__",
   "icons": {
      "128": "images/128x128.png",
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_RewardsTitle__",
   "permissions": [ "cookies", "tabs", "storage", "http://*/*", "https://*/*", "management", "notifications" ],
   "short_name": "__MSG_RewardsShortName__",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "1.3",
   "web_accessible_resources": [ "*.png" ]
}
